#version 330

in vec2 aPos;
in vec4 aClip;
in vec2 aSize;
in vec2 aScale;
in float aRotation;


out VS_OUT {
	vec4 clip;
	vec2 size;
	vec2 scale;
	float rotation;
} vs_out;




void main()
{
    gl_Position = vec4(aPos, 0.0, 1.0);
    vs_out.clip = aClip;
    vs_out.size = aSize;
    vs_out.scale = aScale;
    vs_out.rotation = aRotation;

}
